#include <stdio.h>
int main(){
    int hamburger = 2001, beverage = 2001;
    int h, d;
    for(int i=0; i<3; i++){
        h=0;
        scanf("%d", &h);
        if(h<hamburger)
            hamburger = h;
    }
    for(int i=0; i<2; i++){
        d=0;
        scanf("%d", &d);
        if(d<beverage)
            beverage = d;
    }
    printf("%d\n", hamburger+beverage-50);
    return 0;
}
