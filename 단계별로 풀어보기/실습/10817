#include <stdio.h>
int main(){
    int a,b,c;
    scanf("%d %d %d", &a, &b, &c);
    
    if(a>=b && c>=b){
        if(a>=c)
            printf("%d", c);
        else
            printf("%d", a);
    }
    else if(b>=a && c>=a){
        if(b>=c)
            printf("%d", c);
        else
            printf("%d", b);
    }
    else if(a>=c && b>=c){
        if(a>=b)
            printf("%d", b);
        else
            printf("%d", a);
    }
}
